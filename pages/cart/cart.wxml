<view wx:if="{{!sEmpty}}" class="container">
	<block wx:for="{{cartItems}}" wx:key="index">
		<s-cart-item cart-item="{{item}}" bind:overflow="onOverflow" bind:itemdeelete="onDeleteItem"></s-cart-item>
	</block>
</view>

<view class="empty-container"wx:if="{{isEmpty}}">
	<s-empty show-btn show text="购物车空空如也, 去逛逛吧～" btn-text="去逛逛"></s-empty>
</view>
